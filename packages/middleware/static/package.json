{
  "name": "@hattip/static",
  "version": "0.0.45",
  "type": "module",
  "description": "Static file serving middleware for Hattip",
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./node": "./dist/node.js",
    "./fs": {
      "types": "./dist/fs.d.ts",
      "workerd": "./dist/fs.workerd.js",
      "bun": "./dist/fs.bun.js",
      "deno": "./dist/fs.deno.js",
      "node": "./dist/fs.node.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*.d.ts"
      ]
    }
  },
  "author": "Fatih Ayg√ºn <cyco130@gmail.com>",
  "repository": "https://github.com/hattipjs/hattip",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsup",
    "dev": "tsup --watch",
    "prepack": "pnpm build",
    "test": "pnpm test:typecheck && pnpm test:lint && pnpm test:package",
    "test:typecheck": "tsc -p tsconfig.json --noEmit",
    "test:lint": "eslint . --max-warnings 0 --ignore-pattern dist",
    "test:package": "publint --strict"
  },
  "dependencies": {
    "@hattip/headers": "workspace:*"
  },
  "devDependencies": {
    "@cyco130/eslint-config": "^3.7.1",
    "@types/node": "^20.11.30",
    "eslint": "^9.2.0",
    "publint": "^0.2.7",
    "tsup": "^8.0.2",
    "typescript": "^5.4.3"
  }
}
