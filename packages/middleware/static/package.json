{
  "name": "@hattip/static",
  "version": "0.0.49",
  "type": "module",
  "description": "Static file serving middleware for Hattip",
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./node": "./dist/node.js",
    "./fs": {
      "types": "./dist/fs.d.ts",
      "workerd": "./dist/fs.workerd.js",
      "bun": "./dist/fs.bun.js",
      "deno": "./dist/fs.deno.js",
      "node": "./dist/fs.node.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*.d.ts"
      ]
    }
  },
  "author": "Fatih Ayg√ºn <cyco130@gmail.com>",
  "repository": "github:hattipjs/hattip",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsup",
    "dev": "tsup --watch",
    "prepack": "pnpm build",
    "test": "pnpm test:typecheck && pnpm test:lint && pnpm test:package",
    "test:typecheck": "tsc -p tsconfig.json --noEmit",
    "test:lint": "eslint . --max-warnings 0",
    "test:package": "publint --strict"
  },
  "dependencies": {
    "@hattip/headers": "workspace:*"
  },
  "devDependencies": {
    "@cyco130/eslint-config": "^5.0.1",
    "@types/node": "^18.19.68",
    "eslint": "^9.17.0",
    "publint": "^0.2.12",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  }
}
