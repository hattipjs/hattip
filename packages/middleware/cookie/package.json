{
  "name": "@hattip/cookie",
  "version": "0.0.48",
  "type": "module",
  "description": "Cookie management for Hattip",
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./parse": "./dist/parse.js",
    "./serialize": "./dist/serialize.js"
  },
  "typesVersions": {
