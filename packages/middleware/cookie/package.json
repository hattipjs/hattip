{
  "name": "@hattip/cookie",
  "version": "0.0.19",
  "type": "module",
  "description": "Cookie management for HatTip",
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./parse": "./dist/parse.js",
    "./serialize": "./dist/serialize.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*.d.ts"
      ]
    }
  },
  "author": "Fatih Ayg√ºn <cyco130@gmail.com>",
  "repository": "https://github.com/hattipjs/hattip",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsup",
    "dev": "tsup --watch",
    "prepack": "pnpm build",
    "test": "pnpm test:typecheck && pnpm test:lint && pnpm test:package",
    "test:typecheck": "tsc -p tsconfig.json --noEmit",
    "test:lint": "eslint . --max-warnings 0 --ignore-pattern dist",
    "test:package": "publint"
  },
  "dependencies": {
    "@hattip/compose": "workspace:*",
    "@hattip/core": "workspace:*",
    "@types/cookie": "^0.5.1",
    "cookie": "^0.5.0"
  },
  "devDependencies": {
    "@cyco130/eslint-config": "^2.1.0",
    "@types/node": "^18.0.0",
    "eslint": "^8.18.0",
    "publint": "^0.1.1",
    "tsup": "^6.1.2",
    "typescript": "^4.7.4"
  }
}
