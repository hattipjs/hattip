{
  "name": "@hattip/adapter-cloudflare-workers",
  "version": "0.0.1",
  "description": "Cloudflare Workers adapter for Hattip",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "files": [
    "dist",
    "*.d.ts",
    "default-entry.mjs",
    "static-assets-entry.mjs"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./default-entry": {
      "import": "./default-entry.mjs"
    },
    "./static-assets-entry": {
      "import": "./static-assets-entry.mjs"
    }
  },
  "author": "Fatih Ayg√ºn <cyco130@gmail.com>",
  "repository": "https://github.com/cyco130/vavite",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsup",
    "dev": "tsup --watch",
    "prepack": "pnpm build",
    "test": "pnpm test:typecheck && pnpm test:lint",
    "test:typecheck": "tsc -p tsconfig.json --noEmit",
    "test:lint": "eslint . --max-warnings 0 --ignore-pattern dist"
  },
  "devDependencies": {
    "@cloudflare/kv-asset-handler": "^0.2.0",
    "@cloudflare/workers-types": "^3.4.0",
    "@cyco130/eslint-config": "^1.0.0",
    "@hattip/core": "workspace:*",
    "@types/node": "^17.0.23",
    "eslint": "^8.12.0",
    "tsup": "^5.12.1",
    "typescript": "^4.6.3"
  }
}
