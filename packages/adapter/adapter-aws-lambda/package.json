{
  "name": "@hattip/adapter-aws-lambda",
  "version": "0.0.47",
  "type": "module",
  "description": "AWS Lambda adapter for Hattip",
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./streaming": "./dist/streaming.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*.d.ts"
      ]
    }
  },
  "author": "Fatih Ayg√ºn <cyco130@gmail.com>",
  "repository": "github:hattipjs/hattip",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsup",
    "dev": "tsup --watch",
    "prepack": "pnpm build",
    "test": "pnpm test:typecheck && pnpm test:lint && pnpm test:package",
    "test:typecheck": "tsc -p tsconfig.json --noEmit",
    "test:lint": "eslint . --max-warnings 0",
    "test:package": "publint --strict"
  },
  "devDependencies": {
    "@cyco130/eslint-config": "^3.9.4",
    "@types/node": "^18.19.44",
    "eslint": "^8.57.0",
    "publint": "^0.2.9",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@hattip/adapter-node": "workspace:*",
    "@hattip/core": "workspace:*",
    "@hattip/polyfills": "workspace:*",
    "@types/aws-lambda": "^8.10.143",
    "aws-lambda": "^1.0.7",
    "lambda-stream": "^0.5.0"
  }
}
