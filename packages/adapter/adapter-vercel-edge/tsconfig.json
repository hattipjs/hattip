{
	"compilerOptions": {
		"noEmit": true,
		"target": "es2020",
		"module": "ESNext",
		"lib": ["es2020", "WebWorker"],
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"moduleResolution": "Node",
		"checkJs": true
	},
	"exclude": ["node_modules", "dist"]
}
